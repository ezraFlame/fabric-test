{
  "schemaVersion": 1,
  "id": "fabrictest",
  "version": "${version}",

  "name": "Fabric Test Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "S. C. Arzehd"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.scarzehd.fabrictest.FabricTest"
    ],
    "client": [
      "com.scarzehd.fabrictest.FabricTestClient"
    ]
  },
  "mixins": [
    "fabrictest.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
